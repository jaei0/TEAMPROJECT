<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".services.parking.ParkingCommonService">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:gravity="bottom">

        <ImageButton
            android:id="@+id/p_homebtn"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/p_hmbtn"
            app:tint="#cccccc" />


        <ImageButton
            android:id="@+id/p_searchbtn"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/common_searchbtn1"
            app:tint="#cccccc" />


        <ImageButton
            android:id="@+id/p_bookmarkbtn"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:backgroundTint="@android:color/transparent"
            android:paddingBottom="5dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/common_bookmark_heart2"
            app:tint="#cccccc" />


        <ImageButton
            android:id="@+id/p_identitybtn"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:backgroundTint="@android:color/transparent"
            android:paddingBottom="1dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/p_identity1"
            app:tint="#cccccc" />

    </LinearLayout>
</LinearLayout>